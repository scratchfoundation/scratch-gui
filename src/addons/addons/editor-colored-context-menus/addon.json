{
  "name": "Colored context menus",
  "description": "Makes the editor context menus colorful when right-clicking a block.",
  "credits": [
    {
      "name": "GarboMuffin"
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "injectAsStyleElt": true,
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "userstyles": [
    {
      "url": "userscript.css",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "tags": ["editor", "theme"],
  "enabledByDefault": false,
  "l10n": true
}
